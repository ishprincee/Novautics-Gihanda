RESOURCE_GROUP_NAME="novautics-frontend-rg"
STORAGE_ACCOUNT_NAME="novauticsfrontend01"
LOCATION="uksouth"


#################################

RESOURCE_GROUP_NAME="novautics-frontend-rg"
STORAGE_ACCOUNT_NAME="novauticsfrontend01"
LOCATION="uksouth"

################################

az group create \
  --name $RESOURCE_GROUP_NAME \
  --location $LOCATION

###############################

az storage account create \
  --name $STORAGE_ACCOUNT_NAME \
  --location $LOCATION \
  --resource-group $RESOURCE_GROUP_NAME \
  --sku Standard_LRS
